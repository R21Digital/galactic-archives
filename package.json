{
  "name": "swgdb",
  "version": "1.0.0",
  "type": "module",
  "description": "A unified SWG knowledge hub combining official Restoration Wiki, SWG Fandom data, and exclusive resources powered by The Eye of the Galactic Beholder.",
  "main": ".eleventy.js",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "import:swgr": "node scripts/import_swgr_restoration.js",
    "import:fandom": "node scripts/import_swg_fandom.js",
    "fetch:activity": "node scripts/fetchActivityLog.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "classify": "node scripts/classifyContent.js",
    "classify:enhanced": "node scripts/enhancedClassifier.js",
    "classify:incoming": "node scripts/enhancedClassifier.js --incoming",
    "enhance:alt": "node scripts/altTextEnhancer.js --all",
    "monitor:patches": "node scripts/patchNoteMonitor.js --monitor",
    "export:ms11": "node scripts/ms11Export.js --full",
    "lint": "eslint \"src/**/*.js\" \"scripts/**/*.js\""
  },
  "keywords": [
    "swg",
    "galaxies",
    "wiki",
    "eleventy",
    "static site"
  ],
  "author": "R21 Digital",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.5"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^17.2.1",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3"
  }
}
